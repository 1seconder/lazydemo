	});
	function chenjiao(obj){
			if($(obj).val()==0)
			{
				$("#maimaichengjiao").show();
				$("#zulingchengjiao").hide();
			}
			else if($(obj).val()==1)
			{
				$("#maimaichengjiao").hide();
				$("#zulingchengjiao").show();
			}
		}
</script>