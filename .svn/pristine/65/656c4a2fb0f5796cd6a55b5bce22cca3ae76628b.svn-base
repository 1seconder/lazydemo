<script type="text/javascript">

	$(document).ready(function(){
		
	});
	function chenjiao(obj){
		queryContractForList($(obj).val());
	}
	
	function queryOwnerInfo(pageIndex,flag){
		var type = $("#userTitle").val();
		if(flag == 3){
			type = 0;
		}else{
			if(type == 0){
				$("#ownerList").css("display","none");
				$("#ownerListInfoPag").css("display","none");
			}else{
				$("#singleOwner").css("display","none");
			}
			$("#ownerListInfo").empty();
			$("#ownerBasicInfo").empty();
			$("#otherInfo").empty();
			$("#otherRequirement").empty();
		}
		
		var postParameter = new Object();
		var requestParameter = new Object();
		var pageInfo = new Object();
		pageInfo.page = pageIndex;
		requestParameter.creatorId = $("#yzId").val();
		if(flag == 3){
			requestParameter.creatorId = $("#pId").val();
		}
		requestParameter.type = type;
		postParameter.pageInfo = pageInfo;
		postParameter.requestParameter = requestParameter;
		var dataJson = JSON.stringify(postParameter);
		var url = "<%=basePath%>/services/houseService/queryOwnerInfo";
		var result = $.ajax({
			url : url,
			type : "POST",
			data : dataJson,
			contentType : "application/json;charset=UTF-8",
			dataType : "json",
			async : false
		});
		if(type == 0){
			var resultArr = $.parseJSON(result.responseText);
			if(null == resultArr || typeof(resultArr) == "undefined" || resultArr.length == 0){
				
			}else{
				var resultData = resultArr[0];
				var gender = resultData.gender == 2 ? "女":"男";
				var isgroupcustomer = resultData.isgroupcustomer == 0 ? "否":"是";
				var nationality = resultData.nationality  == null ? "":resultData.nationality ;
				var DelegateType = resultData.delegatetype  == 1 ? "求购":"求租";
				var fullname = resultData.fullname == null ? '':resultData.fullname;
				var mobilephone = resultData.mobilephone == null ? '':resultData.mobilephone;
				var cityname = resultData.cityname == null ? '':resultData.cityname;
				var requirecity = resultData.requirecity == null ? '':resultData.requirecity;
				var qy_name = resultData.qy_name == null ? '':resultData.qy_name;
				var isqualifiedtobuyhouse = resultData.isqualifiedtobuyhouse == null ? '':resultData.isqualifiedtobuyhouse;
				var workplace = resultData.workplace == null ? '':resultData.workplace;
				var education = resultData.education == null ? '':resultData.education;
				var birthday = resultData.birthday == null ? '':resultData.birthday;
				var consumptionconcept = resultData.consumptionconcept == null ? '':resultData.consumptionconcept;
				var worktype = resultData.worktype == null ? '':resultData.worktype;
				var ownedhousenumber = resultData.ownedhousenumber == null ? '':resultData.ownedhousenumber;
				var customerlevel = resultData.customerlevel == null ? '':resultData.customerlevel;
				var communicationstage = resultData.communicationstage == null ? '':resultData.communicationstage;
				
				var taxpaymentlimit = resultData.taxpaymentlimit == null ? '':resultData.taxpaymentlimit;
				var socialityinsurancepaymentlimit = resultData.socialityinsurancepaymentlimit == null ? '':resultData.socialityinsurancepaymentlimit;
				var housingfundremainamount = resultData.housingfundremainamount == null ? '':resultData.housingfundremainamount;
				var housingfundpaymentratioofmonth = resultData.housingfundpaymentratioofmonth == null ? '':resultData.housingfundpaymentratioofmonth ;
				var housingfundpaymentamountofmonth = resultData.housingfundpaymentamountofmonth == null ? '':resultData.housingfundpaymentamountofmonth;
				var housingfundpaymentlimit = resultData.housingfundpaymentlimit == null ? '':resultData.housingfundpaymentlimit;
				var isfamiliartocommunity = resultData.isfamiliartocommunity == null ? '':resultData.isfamiliartocommunity;
				var transport = resultData.transport == null ? '':resultData.transport;
				var payingtype = result.payingtype == null ? '':resultData.payingtype;
