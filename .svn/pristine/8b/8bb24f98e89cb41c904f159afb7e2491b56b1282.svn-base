<script type="text/javascript">
	jQuery(document).ready(function($) {
		$("#timeCondition").val(1);
		queryList(1);
		queryLpxxForList();
	});
	
	function chooseCondition(id,value,obj){
		$("#"+id).parent().find("a").removeClass("active");
		$(obj).addClass("active");
		$("#"+id+"Condition").val(value);
	}
	
	function queryList(pageIndex){
		$("#lpjgInfoList").empty();
		var postParameter = new Object();
		var requestParameter = new Object();
		var pageInfo = new Object();
		pageInfo.page = pageIndex;
		requestParameter.pageInfo = pageInfo;
		requestParameter.timeStatus = $("#timeCondition").val();
		postParameter.requestParameter = requestParameter;
		var dataJson = JSON.stringify(postParameter);
		
		var url = "<%=basePath%>/services/houseService/querylpjgInfoForList";
		
		var result = $.ajax({
			url : url,
			data : dataJson,
			dataType : "json",
			type : "POST",
			contentType : "application/json;charset=UTF-8",
			async : false
		});
		
		var resultArr = result.responseJSON.listJson.gridModel;
		var html = "";
		if(null == resultArr || typeof(resultArr) == "undefined"){
			
		}else{
			for(var i = 0;i< resultArr.length; i++){
				var resultData = resultArr[i];
				html +='<tr><td class="col-cb middle-align text-center"><input type="checkbox" class="cbr" /></td>'+
				'<td class="col-lg-1">'+resultData.dzName+'</td>'+
				'<td>'+resultData.belongerid+'</td>'+
				'<td class="middle-align text-center">'+resultData.lpName+'/'+resultData.dzName+'/'+resultData.dyName+'/'+resultData.fhid+'</td>'+
				'<td class="middle-align text-center">'+resultData.dzName+'</td>'+
				'<td class="middle-align text-center">'+resultData.dzName+'</td>'+
				'<td class="middle-align text-center">'+resultData.dzName+'</td>'+
				'<td class="middle-align text-center">'+resultData.dzName+'</td>'+
				'<td class="middle-align text-center">'+resultData.createdate+'</td>'+
				'<td class="middle-align text-center"><a class="btn btn-danger btn-xs">跟进</a><a class="btn btn-danger btn-xs">无效</a></td></tr>';
			}
			$("#lpjgInfoList").append(html);
		}
	}
	
	function queryLpxxForList(){
		$("#lpxxSelect").empty();
		var requestParameter = new Object();
		var url = "<%=basePath%>/services/houseService/queryLpxxForList";
		var jsonData = JSON.stringify(requestParameter);
		var result = $.ajax({
			url : url,
			data : jsonData,
			type : "POST",
			dataType : "json",
			contentType : "application/json;charset=UTF-8",
			async : false
		});
		
		var resultArr = result.responseJSON;
		var html ="";
		
		if(null == resultArr || typeof(resultArr) == "undefined"){
			
		}else{
			for(var i=0;i<resultArr.length;i++){
				var resultData = resultArr[i];
				html += "<option id='"+resultData.id+"'>"+result.lp_name+"</option>";
			}
			$("#lpxxSelect").append(html);
		}
		
	}
	
	function queryLpxxForList(){
		$("#lpxxSelect").select2({
			minimumInputLength: 1,
			placeholder: '请输入小区名称搜索',
			ajax: {
				url: "<%=basePath%>/services/houseService/queryLpxxForList",
				type: "GET",
				dataType: 'json',
				quietMillis: 100,
				data: function(term, page) {
					return {
						"keyWord": term
					};
				},
				results: function(data, page ) {
					return { results: data.result }
				}
			},
			formatResult: function(obj) { 
				return "<div class='select2-user-result'>" + obj.lp_name+"</div>";
			},
			formatSelection: function(obj) {
				return obj.lp_name;
				$("#lpxxSelect").val(obj.id);
			}
		}).on("change",function(e){
			
		});
		
	}
</script>