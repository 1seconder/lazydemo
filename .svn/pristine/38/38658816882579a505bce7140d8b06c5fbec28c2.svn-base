package com.newenv.communityFocus.domain.jinggeng;



import org.nutz.dao.entity.annotation.Table;

/**
 * @author hui.peng
 */
@Table("lpjg_customerrequirementofrent")
public class CustomerRequirementOfRent  {
	
	private int customerId;
	
	private int rentingRange;//租期 
	private int rentType ;//租赁类型
	
	private int IsTransfer ;//是否可转让 
	
	
	private int payType; //fuk 
	
	private int livePersonCount; //
	
	private int mortgage;//押
	private int fee; //付

	public int getCustomerId() {
		return customerId;
	}

	public void setCustomerId(int customerId) {
		this.customerId = customerId;
	}

	public int getRentingRange() {
		return rentingRange;
	}

	public void setRentingRange(int rentingRange) {
		this.rentingRange = rentingRange;
	}

	public int getRentType() {
		return rentType;
	}

	public void setRentType(int rentType) {
		this.rentType = rentType;
	}

	public int getIsTransfer() {
		return IsTransfer;
	}

	public void setIsTransfer(int isTransfer) {
		IsTransfer = isTransfer;
	}

	public int getPayType() {
		return payType;
	}

	public void setPayType(int payType) {
		this.payType = payType;
	}

	public int getLivePersonCount() {
		return livePersonCount;
	}

	public void setLivePersonCount(int livePersonCount) {
		this.livePersonCount = livePersonCount;
	}

	public int getMortgage() {
		return mortgage;
	}

	public void setMortgage(int mortgage) {
		this.mortgage = mortgage;
	}

	public int getFee() {
		return fee;
	}

	public void setFee(int fee) {
		this.fee = fee;
	}

	

	
	
	
}