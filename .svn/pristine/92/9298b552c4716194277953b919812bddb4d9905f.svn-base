<script type="text/javascript">
	$('.form_date').datetimepicker({
		//$('.form_date').datetimepicker({ “form_datetime” 时分秒
		language: 'zh-CN',
		weekStart: 1,
		todayBtn: 1,
		autoclose: 1,
		todayHighlight: 1,
		startView: 2,
		minView: 2,
		forceParse: 0
	});
	$(".form_datetime").datetimepicker({
		format: 'yyyy-mm-dd:hh:ii',
		language: 'zh-CN',
	});
	function xzjgneiron(obj){
		if($(obj).val()==0)
		{
			$("#weituofanyuan").show();
			$("#weituoyaoshi").hide();
			$("#weituoqifankan").hide();
			$("#weituodujia").hide();
		}
		else if($(obj).val()==1)
		{
			$("#weituofanyuan").hide();
			$("#weituoyaoshi").show();
			$("#weituoqifankan").hide();
			$("#weituodujia").hide();
		}
		else if($(obj).val()==2)
		{
			$("#weituofanyuan").hide();
			$("#weituoyaoshi").hide();
			$("#weituoqifankan").show();
			$("#weituodujia").hide();
		}
		else if($(obj).val()==3)
		{
			$("#weituofanyuan").hide();
			$("#weituoyaoshi").hide();
			$("#weituoqifankan").hide();
			$("#weituodujia").show();
		}
	}
	function jiecujingen(obj){
		if($(obj).val()==0)
		{
			$("#jc_jibenxinxi").show();
			$("#jc_shanquanjinli").hide();
			$("#jc_yezujinli").hide();
			$("#jc_dianhuagenjin").hide();
			$("#jc_duishougenjin").hide();
		}
		else if($(obj).val()==1)
		{
			$("#jc_jibenxinxi").hide();
			$("#jc_shanquanjinli").show();
			$("#jc_yezujinli").hide();
			$("#jc_dianhuagenjin").hide();
			$("#jc_duishougenjin").hide();
		}
		else if($(obj).val()==2)
		{
			$("#jc_jibenxinxi").hide();
			$("#jc_shanquanjinli").hide();
			
			var par = new Object();
			par.type = 2;
			par.fhs = 197642;
			var dataJson = JSON.stringify(par);
			$.ajax({
				url: "<%=basePath%>/services/refineService/pageJump/",
				dataType : "json",
				type : "POST",
				data : dataJson ,
				contentType : "application/json;charset=UTF-8",
				async : false,
				success: function(data){
		 			//alert("操作成功");
		 			var c = '';
		 			$.each(data.yt,function(n,v) {   
		 				c += '<option value="'+ v.id + '">'+ v.name +'</option>';
		 			})
		 			$("#useType").html(c);
		 		}
			})
			
			$("#jc_yezujinli").show();
			$("#jc_dianhuagenjin").hide();
			$("#jc_duishougenjin").hide();
		}
		else if($(obj).val()==3)
		{
			$("#jc_jibenxinxi").hide();
			$("#jc_shanquanjinli").hide();
			$("#jc_yezujinli").hide();
			$("#jc_dianhuagenjin").show();
			$("#jc_duishougenjin").hide();
		}
		else if($(obj).val()==4)
		{
			$("#jc_jibenxinxi").hide();
			$("#jc_shanquanjinli").hide();
			$("#jc_yezujinli").hide();
			$("#jc_dianhuagenjin").hide();
			$("#jc_duishougenjin").show();
		}
	}
</script>
