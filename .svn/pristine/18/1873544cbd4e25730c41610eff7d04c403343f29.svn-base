.sea-form .sea-top .sea-soo em{ width: 28px; height:24px; border-right: 1px solid #dfdfdf; position: absolute; left:0px; top: 0px; background-position: -13px 7px; text-indent: -999999px;}
.sea-form .sea-top .sea-soo a{ width: 52px; height: 24px; background:#16b701; border: 1px solid #009207; position: absolute; right: -52px; top: -1px; border-radius: 3px; text-align: center; color: #fff;}
.sea-form .sea-top .sea-soo a:hover{ background:#1ec908;}

/* fysearch */
.fysearch ul li{ padding: 7px 13px; line-height: 24px; position: relative; border-bottom: 1px solid #d3f0dd;}
